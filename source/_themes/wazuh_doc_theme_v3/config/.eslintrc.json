{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len": [
            "error", {
                "code": 120,
                "ignoreUrls": true,
                "ignoreStrings": true
            }
        ]
    }
}
